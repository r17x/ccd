{
  "manifest_version": 3,
  "name": "CCD",
  "author": "r17x",
  "version": "6.6.6",
  "content_scripts": [
    {
      "matches": [
        "https://*.darwinbox.com/**"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {},
  "background": {
    "service_worker": "background.js"
  }
}
